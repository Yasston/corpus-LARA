      <document>Haie.html</document>
    </metadata>
    <SE>
      <id>3</id>
      <text>Par ailleurs, les haies composées d’arbres fourragers (frênes, robiniers, ormes, érables, mûriers)</text>
      <span begin="9720" end="9818" />
    </SE>
    <comment />
  </annotation>
