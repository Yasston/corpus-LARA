    </metadata>
    <SE>
      <id>2</id>
      <text>chants (Hymnes et louanges, gospel, etc.)</text>
      <span begin="1906" end="1947" />
    </SE>
    <comment />
  </annotation>
  <annotation type="1">
    <metadata>
      <id>3</id>
