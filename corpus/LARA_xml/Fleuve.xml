    <metadata>
      <id>2</id>
      <auteur>Autre</auteur>
      <date>4/01/14 22:21</date>
      <document>Fleuve.html</document>
    </metadata>
    <SE>
      <id>2</id>
      <text>Dans certains contexte (sols et substrats perméables ou semi-perméables),</text>
      <span begin="2349" end="2423" />
    </SE>
    <comment />
  </annotation>
